page {
  padding-bottom: 24rpx;
  background: url(data:image/png;base64,) no-repeat left top/360rpx auto, linear-gradient(180deg, #D4FFDF 160rpx, var(--page-bg) 510rpx);
}
.user-wrapper,
.box-wrapper,
.main-menus,
.banner,
.common-wrapper {
  margin-left: 24rpx;
  margin-right: 24rpx;
}
.user-wrapper {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 22rpx 12rpx 0 8rpx;
  margin-bottom: 40rpx;
}
.user-wrapper .left {
  display: flex;
  align-items: center;
}
.user-wrapper .left .avatar {
  overflow: hidden;
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
  border: 4rpx solid #FFFFFF;
  margin-right: 24rpx;
  background-color: #C8D4DF;
}
.user-wrapper .left .user .name {
  font-weight: bold;
  font-size: 40rpx;
  color: var(--title-color);
  line-height: 40rpx;
  margin-bottom: 8rpx;
}
.user-wrapper .left .user .phone {
  font-size: 26rpx;
  color: var(--content-color);
  line-height: 36rpx;
}
.user-wrapper .left .no-login {
  display: flex;
  align-items: center;
  font-weight: bold;
  font-size: 40rpx;
  color: var(--title-color);
  line-height: 40rpx;
}
.user-wrapper .right {
  display: flex;
  align-items: center;
}
.user-wrapper .right .btn {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 72rpx;
  height: 72rpx;
  background: #FFFFFF;
  border-radius: 24rpx;
  margin-left: 28rpx;
}
.box-wrapper {
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.box-wrapper .box {
  display: flex;
  flex: 1;
  height: 136rpx;
  border-radius: 24rpx;
  overflow: hidden;
  background-color: #FFFFFF;
}
.box-wrapper .box:not(:last-child) {
  margin-right: 18rpx;
}
.box-wrapper .box .name {
  flex: none;
  width: 178rpx;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-weight: bold;
  font-size: 30rpx;
  color: var(--title-color);
}
.box-wrapper .box .icon {
  flex: 1;
  background-color: #C8D4DF;
}
.main-menus {
  border-radius: 24rpx;
  background-color: #FFFFFF;
  padding: 32rpx 0 28rpx;
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.main-menus .menu {
  flex: 1;
  display: flex;
  align-items: center;
  flex-direction: column;
  flex: 1;
  font-size: 26rpx;
  color: var(--title-color);
  line-height: 36rpx;
}
.banner {
  height: 192rpx;
  border-radius: 24rpx 24rpx 24rpx 24rpx;
  margin-bottom: 20rpx;
}
.common-wrapper {
  padding: 28rpx 0;
  background: #FFFFFF;
  border-radius: 24rpx;
  color: var(--title-color);
  line-height: 40rpx;
}
.common-wrapper .title {
  padding: 0 28rpx;
  font-weight: bold;
  margin-bottom: 32rpx;
  font-size: 32rpx;
}
.common-wrapper .menus {
  display: flex;
  flex-wrap: wrap;
}
.common-wrapper .menus .menu {
  line-height: 40rpx;
  font-size: 24rpx;
  flex: none;
  width: 25%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 32rpx;
  background-color: transparent;
}
.common-wrapper .menus .menu::after {
  display: none;
}