page {
  padding: 40rpx 24rpx 20rpx;
}
.page-bg {
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
  height: 718rpx;
  width: 100%;
  background: linear-gradient(to bottom, transparent 168px, #F2F4F7), url(data:image/png;base64,), linear-gradient(270deg, #31CE57 0%, #07B130 100%);
}
.dispatch-wrapper {
  margin-bottom: 20rpx;
}
.dispatch-wrapper .task-wrapper {
  margin-top: -28rpx;
  position: relative;
  height: 160rpx;
  background: url(data:image/png;base64,) left bottom/auto 108rpx no-repeat, url(data:image/png;base64,) right bottom/auto 160rpx no-repeat;
}
.dispatch-wrapper .task-wrapper::after {
  content: "";
  display: block;
  height: 42rpx;
  width: 100%;
  position: absolute;
  z-index: -1;
  left: 0;
  bottom: 0;
  background: #E7F9E9;
  border-radius: 32rpx 32rpx 0rpx 0rpx;
}
.dispatch-wrapper .task-wrapper .content {
  display: flex;
  align-items: center;
  position: absolute;
  left: 24rpx;
  bottom: 0;
  width: -webkit-fit-content;
  width: fit-content;
  padding: 24rpx 0 24rpx 24rpx;
  border-radius: 16rpx 0rpx 0rpx 16rpx;
  font-weight: bold;
  font-size: 28rpx;
  line-height: 36rpx;
}
.dispatch-wrapper .task-wrapper .content.green {
  background: linear-gradient(90deg, #E7F9E9 65%, #FFFFFF 100%);
  color: var(--main-color);
}
.dispatch-wrapper .task-wrapper .content.orange {
  color: #FC7E2C;
  background: linear-gradient(90deg, #FFF1E1 65%, rgba(255, 242, 233, 0) 100%);
}
.dispatch-wrapper .form-wrapper {
  background-color: #FFFFFF;
  padding: 24rpx;
  border-radius: 0 0 24rpx 24rpx;
}
.dispatch-wrapper .form-wrapper .select-location .from-wrapper,
.dispatch-wrapper .form-wrapper .select-location .to-wrapper {
  padding: 0 24rpx;
  display: flex;
  align-items: center;
  height: 120rpx;
  background: var(--page-bg);
  border-radius: 16rpx;
}
.dispatch-wrapper .form-wrapper .select-location .from-wrapper .icon,
.dispatch-wrapper .form-wrapper .select-location .to-wrapper .icon {
  flex: none;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 36rpx;
  height: 36rpx;
  border-radius: 8rpx;
  font-weight: 600;
  font-size: 22rpx;
  color: #FFFFFF;
}
.dispatch-wrapper .form-wrapper .select-location .from-wrapper .info,
.dispatch-wrapper .form-wrapper .select-location .to-wrapper .info {
  flex: 1;
  margin: 0 24rpx;
  font-size: 32rpx;
  font-weight: bold;
  line-height: 48rpx;
}
.dispatch-wrapper .form-wrapper .select-location .from-wrapper .info .placeholder,
.dispatch-wrapper .form-wrapper .select-location .to-wrapper .info .placeholder {
  color: var(--intr-color);
}
.dispatch-wrapper .form-wrapper .select-location .from-wrapper .info .name,
.dispatch-wrapper .form-wrapper .select-location .to-wrapper .info .name {
  color: var(--title-color);
}
.dispatch-wrapper .form-wrapper .select-location .from-wrapper {
  margin-bottom: 24rpx;
}
.dispatch-wrapper .form-wrapper .select-location .from-wrapper .icon {
  background-color: var(--main-color);
}
.dispatch-wrapper .form-wrapper .select-location .to-wrapper .icon {
  background-color: #FC7E2C;
}
.dispatch-wrapper .form-wrapper .item {
  padding: 36rpx 16rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  line-height: 48rpx;
}
.dispatch-wrapper .form-wrapper .item .label {
  font-size: 28rpx;
  color: var(--content-color);
}
.dispatch-wrapper .form-wrapper .tip {
  text-align: center;
  font-size: 24rpx;
  color: var(--intr-color);
  line-height: 32rpx;
}
.menus {
  border-radius: 24rpx;
  background-color: #FFFFFF;
  padding: 32rpx 0;
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.menus .menu {
  flex: 1;
  display: flex;
  align-items: center;
  flex-direction: column;
  flex: 1;
  font-size: 26rpx;
  color: var(--title-color);
  line-height: 36rpx;
}
.banner {
  height: 192rpx;
  background: #73838E;
  border-radius: 24rpx 24rpx 24rpx 24rpx;
}